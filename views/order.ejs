<!DOCTYPE html>
<html>
<% include header.ejs %>
<body ontouchstart="">
    <div class="container">
        <div class="hd">
            <h1 class="page_title"><%= title %></h1>
        </div>
        <div class="bd">
            <div class="weui_cells_form">
                <div class="weui_cells">
                <div class="weui_cells_title">保险类型</div>
                <div class="weui_cell weui_cell_select">
                    <div class="weui_cell_bd weui_cell_primary">
                        <select class="weui_select" name="ctl00$TopIssues$ServiceId" id="TopIssues_ServiceId">
                            <%insuranceFormInfoSelect.TopIssues_ServiceId.options.forEach(function(option, index){%>
                                <option value='<%=option.value%>' <%=option.selected%>><%=option.text%></option>
                            <%})%>
                        </select>
                    </div>
                </div>
                <div class="weui_cell weui_cell_select">
                    <div class="weui_cell_bd weui_cell_primary">
                        <select class="weui_select" name="ctl00$TopIssues$ProductId" id="TopIssues_ProductId">
                            <%insuranceFormInfoSelect.TopIssues_ProductId.options.forEach(function(option, index){%>
                                <option value='<%=option.value%>' <%=option.selected%>><%=option.text%></option>
                            <%})%>
                        </select>
                    </div>
                </div>
                </div>
                <div class="weui_cells">
                    <div class="weui_cells_title">被保险人</div>
                    <div class="weui_cell">
                        <div class="weui_cell_hd"><label for="" class="weui_label">姓名</label></div>
                        <div class="weui_cell_bd weui_cell_primary">
                            <input class="weui_input" type="text" value="">
                        </div>
                    </div>
                    <div class="weui_cell weui_cells_radio">
                        <div class="weui_cell_hd"><label for="" class="weui_label">姓别</label></div>
                        <div class="weui_cell_bd">
                        <label class="weui_cell weui_check_label" for="MainIssues_InsureSex_0">
                            <div class="weui_cell_bd">
                                <p>男</p>
                            </div>
                            <div class="weui_cell_ft">
                                <input type="radio" name="ctl00$MainIssues$InsureSex" class="weui_check" 
                                id="MainIssues_InsureSex_0" value="<%=insuranceFormInfoSelect.MainIssues_InsureSex_0.value%>"
                                <%=insuranceFormInfoSelect.MainIssues_InsureSex_0.checked%>>
                                <span class="weui_icon_checked"></span>
                            </div>
                        </label>
                        <label class="weui_cell weui_check_label" for="MainIssues_InsureSex_1">
                            <div class="weui_cell_bd">
                                <p>女</p>
                            </div>
                            <div class="weui_cell_ft">
                                <input type="radio" name="ctl00$MainIssues$InsureSex" class="weui_check" 
                                id="MainIssues_InsureSex_1" value="<%=insuranceFormInfoSelect.MainIssues_InsureSex_1.value%>"
                                <%=insuranceFormInfoSelect.MainIssues_InsureSex_1.checked%>>
                                <span class="weui_icon_checked"></span>
                            </div>
                        </label>
                        </div>
                    </div>
                    <div class="weui_cell weui_cell_select weui_select_after">
                        <div class="weui_cell_hd">
                            <label for="" class="weui_label">证件类型</label>
                        </div>
                        <div class="weui_cell_bd weui_cell_primary">
                            <select class="weui_select" name="ctl00$TopIssues$InsureIdType" id="MainIssues_InsureIdType">
                                <%insuranceFormInfoSelect.MainIssues_InsureIdType.options.forEach(function(option, index){%>
                                    <option value='<%=option.value%>' <%=option.selected%>><%=option.text%></option>
                                <%})%>
                            </select>
                        </div>
                    </div>
                    <div class="weui_cell">
                        <div class="weui_cell_hd"><label for="" class="weui_label">证件号码</label></div>
                        <div class="weui_cell_bd weui_cell_primary">
                            <input class="weui_input" type="text" name="ctl00$MainIssues$InsureIdNumber" 
                            id="MainIssues_InsureIdNumber" value="">
                        </div>
                    </div>
                    <div class="weui_cell weui_cell_select weui_select_after">
                        <div class="weui_cell_hd">
                            <label for="" class="weui_label">保险期限</label>
                        </div>
                        <div class="weui_cell_bd weui_cell_primary">
                            <select class="weui_select" name="ctl00$TopIssues$InsureDays" id="MainIssues_InsureDays">
                                <%insuranceFormInfoSelect.MainIssues_InsureDays.options.forEach(function(option, index){%>
                                    <option value='<%=option.value%>' <%=option.selected%>><%=option.text%></option>
                                <%})%>
                            </select>
                        </div>
                    </div>
                    <div class="weui_cell">
                        <div class="weui_cell_hd"><label for="" class="weui_label">出生日期</label></div>
                        <div class="weui_cell_bd weui_cell_primary">
                            <input class="weui_input" type="date" name="ctl00$MainIssues$InsureBirthDate" 
                            id="MainIssues_InsureBirthDate" value="" placeholder="">
                        </div>
                    </div>
                    <div class="weui_cell">
                        <div class="weui_cell_hd"><label for="" class="weui_label">手机号码</label></div>
                        <div class="weui_cell_bd weui_cell_primary">
                            <input class="weui_input" type="number" pattern="[0-9]*" name="ctl00$MainIssues$InsurePhone" 
                            id="MainIssues_InsurePhone" value="" placeholder="">
                        </div>
                    </div>
                    <div class="weui_cell">
                        <div class="weui_cell_hd"><label for="" class="weui_label">航班号</label></div>
                        <div class="weui_cell_bd weui_cell_primary">
                            <input class="weui_input" type="text" name="ctl00$MainIssues$InsureMark" 
                            id="MainIssues_InsureMark" value="" placeholder="">
                        </div>
                    </div>
                    <div class="weui_cell">
                        <div class="weui_cell_hd"><label for="" class="weui_label">承保日期</label></div>
                        <div class="weui_cell_bd weui_cell_primary">
                            <input class="weui_input" type="datetime-local" name="ctl00$MainIssues$InsureBeginTime" 
                            id="MainIssues_InsureBeginTime" value="" placeholder="">
                        </div>
                    </div>
                </div>

                
            </div>
            <!-- <div class="weui_cells">
                <%Object.keys(insuranceFormInfoDefault).forEach(function(key){%>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p><%= key %></p>
                    </div>
                    <div class="weui_cell_ft"><%= insuranceFormInfoDefault[key] %></div>
                </div>
                <%})%>
            </div> -->
        </div>

    </div>
    <% include footer.ejs %>
</body>
</html>
